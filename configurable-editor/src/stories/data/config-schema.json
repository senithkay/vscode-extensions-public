{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "dilhashanazeer": {
            "type": "object",
            "properties": {
                "simpleconfigs": {
                    "type": "object",
                    "properties": {
                        "itemCode": {
                            "type": "string",
                            "description": "Sample description of the itemCode configurable."
                        },
                        "discount": {
                            "type": "number",
                            "description": "Sample description of the discount configurable."
                        },
                        "testMode": {
                            "type": "boolean",
                            "description": "Sample description of the testMode configurable."
                        },
                        "mod1": {
                            "type": "object",
                            "properties": {
                                "nameVal": {
                                    "type": "string",
                                    "description": "Sample description of the nameVal configurable."
                                }
                            }
                        },
                        "person": {
                            "type": "object",
                            "properties": {
                                "name": {
                                    "type": "string"
                                },
                                "age": {
                                    "type": "integer"
                                },
                                "category": {
                                    "type": "object",
                                    "properties": {
                                        "fullTime": {
                                            "type": "boolean"
                                        }
                                    },
                                    "additionalProperties": false,
                                    "description": "Sample description of the category configurable."
                                }
                            },
                            "additionalProperties": false,
                            "description": "Sample description of the person configurable."
                        },
                        "prices": {
                            "type": "array",
                            "items": {
                                "type": "integer"
                            },
                            "description": "Sample description of the prices configurable."
                        },
                        "mapValueString": {
                            "type": "object",
                            "additionalProperties": {
                                "type": "string"
                            },
                            "description": "Sample description of the mapValueString configurable."
                        },
                        "mapValueInteger": {
                            "type": "object",
                            "additionalProperties": {
                                "type": "integer"
                            },
                            "description": "Sample description of the mapValueInteger configurable."
                        },
                        "mapValueIntBoolean": {
                            "type": "object",
                            "additionalProperties": {
                                "type": "boolean"
                            },
                            "description": "Sample description of the mapValueIntBoolean configurable."
                        },
                        "mapValueRecord": {
                            "type": "object",
                            "additionalProperties": {
                                "type": "object",
                                "properties": {
                                    "age": {
                                        "type": "integer"
                                    },
                                    "name": {
                                        "type": "string"
                                    }
                                },
                                "additionalProperties": false,
                                "required": [
                                    "age",
                                    "name"
                                ]
                            },
                            "description": "Sample description of the mapValueRecord configurable."
                        },
                        "mapValueUnion": {
                            "type": "object",
                            "additionalProperties": {
                                "anyOf": [
                                    {
                                        "type": "object",
                                        "properties": {
                                            "age": {
                                                "type": "integer"
                                            },
                                            "name": {
                                                "type": "string"
                                            }
                                        },
                                        "additionalProperties": false,
                                        "required": [
                                            "age",
                                            "name"
                                        ]
                                    },
                                    {
                                        "type": "string"
                                    }
                                ]
                            },
                            "description": "Sample description of the mapValueUnion configurable."
                        }
                    },
                    "required": [
                        "testMode"
                    ]
                }
            }
        }
    }
}

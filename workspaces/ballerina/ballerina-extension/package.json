{
    "name": "kolab",
    "displayName": "KolaB",
    "description": "KolaB Extension for Kola.",
    "version": "0.0.1",
    "publisher": "wso2",
    "homepage": "https://wso2.com/ballerina/vscode/docs",
    "bugs": {
        "url": "https://github.com/wso2/ballerina-plugin-vscode/issues"
    },
    "qna": "https://discord.com/invite/ballerinalang",
    "galleryBanner": {
        "color": "#e7eaeb",
        "theme": "light"
    },
    "engines": {
        "vscode": "^1.83.1"
    },
    "categories": [
        "Programming Languages",
        "Debuggers",
        "Formatters",
        "Snippets"
    ],
    "activationEvents": [
        "onLanguage:ballerina",
        "onCommand:kolab.showExamples",
        "workspaceContains:**/Ballerina.toml",
        "onNotebook:kolab-notebook",
        "onUri"
    ],
    "contributes": {
        "languages": [
            {
                "id": "ballerina",
                "aliases": [
                    "Ballerina",
                    "ballerina"
                ],
                "extensions": [
                    ".bal"
                ],
                "configuration": "./ballerina.configuration.json",
                "icon": {
                    "light": "./resources/images/icons/ballerina.svg",
                    "dark": "./resources/images/icons/ballerina-inverse.svg"
                }
            },
            {
                "id": "toml",
                "aliases": [
                    "Toml",
                    "toml"
                ],
                "extensions": [
                    ".toml"
                ]
            },
            {
                "id": "x-ballerina-output-log"
            }
        ],
        "grammars": [
            {
                "language": "ballerina",
                "scopeName": "source.ballerina",
                "path": "./grammar/ballerina-grammar/syntaxes/ballerina.tmLanguage"
            }
        ],
        "configuration": [
            {
                "type": "object",
                "title": "Kola B",
                "properties": {
                    "kolab.pluginDevMode": {
                        "type": "boolean",
                        "default": false,
                        "description": "Enable plugin development mode."
                    },
                    "kolab.home": {
                        "type": "string",
                        "default": "",
                        "description": "Specify the Ballerina home directory path."
                    },
                    "kolab.debugLog": {
                        "type": "boolean",
                        "default": false,
                        "description": "Enable printing debug messages on to the Ballerina output channel."
                    },
                    "kolab.enableLiveReload": {
                        "type": "boolean",
                        "default": true,
                        "description": "Enable automatic re-run of Ballerina service projects upon file changes."
                    },
                    "kolab.traceLog": {
                        "type": "boolean",
                        "default": false,
                        "description": "Enable printing trace messages on to the Ballerina output channel."
                    },
                    "kolab.codeLens.all.enabled": {
                        "type": "boolean",
                        "default": true,
                        "description": "Enable all code lens features."
                    },
                    "kolab.enableFileWatcher": {
                        "type": "boolean",
                        "default": true,
                        "description": "Enable watching file change events of the project."
                    },
                    "kolab.enableTelemetry": {
                        "type": "boolean",
                        "default": true,
                        "markdownDescription": "Enable Ballerina telemetry."
                    },
                    "kolab.enableSemanticHighlighting": {
                        "type": "boolean",
                        "default": true,
                        "description": "Enable semantic highlighting for Ballerina source."
                    },
                    "kolab.enablePerformanceForecast": {
                        "type": "boolean",
                        "default": true,
                        "description": "Enable real-time performance forecasting."
                    },
                    "kolab.enableLanguageServerDebug": {
                        "type": "boolean",
                        "default": false,
                        "description": "Enable Language Server debug mode."
                    },
                    "kolab.experimental": {
                        "type": "boolean",
                        "default": false,
                        "description": "Enable experimental features."
                    },
                    "kolab.enableNotebookDebug": {
                        "type": "boolean",
                        "default": false,
                        "description": "Enable notebook debug mode."
                    },
                    "kolab.enableRunFast": {
                        "type": "boolean",
                        "default": false,
                        "description": "Experimental: Enable compiling & running Ballerina programs inside Language Server resulting in faster execution."
                    },
                    "kolab.samplesDownloadDirectory": {
                        "type": "string",
                        "default": "",
                        "description": "Specify the directory where you want to download the Ballerina samples. Leave it empty to use the default download directory."
                    },
                    "kolab.enableInlayHints": {
                        "type": "boolean",
                        "default": false,
                        "description": "Experimental: Enable inlay hints for Ballerina."
                    },
                    "kolab.rootUrl": {
                        "type": "string",
                        "default": "https://dev-tools.wso2.com/ballerina-copilot/v2.0",
                        "description": "Ballerina AI Backend service"
                    },
                    "kolab.authOrg": {
                        "type": "string",
                        "default": "ballerinacopilot",
                        "description": "Asgardeo Organization for Auth Services\nDefault: ballerinacopilot"
                    },
                    "kolab.authClientID": {
                        "type": "string",
                        "default": "9rKng8hSZd0VkeA45Lt4LOfCp9Aa",
                        "description": "Client ID of the Asgardeo Organization for Auth Services\n Default: 9rKng8hSZd0VkeA45Lt4LOfCp9Aa"
                    },
                    "kolab.authRedirectURL": {
                        "type": "string",
                        "default": "vscode://wso2.ballerina/signin",
                        "description": "Redirect URL for Auth Service \n Default: vscode://wso2.ballerina/signin"
                    }
                }
            },
            {
                "type": "object",
                "title": "Kola",
                "properties": {
                    "kolab.enableSequenceDiagramView": {
                        "type": "boolean",
                        "default": false,
                        "description": "Enable the experimental Sequence Diagram View."
                    },
                    "kolab.enableAiSuggestions": {
                        "type": "boolean",
                        "default": true,
                        "description": "Enable AI suggestions from WSO2 Copilot."
                    }
                }
            }
        ],
        "configurationDefaults": {
            "[ballerina]": {
                "editor.wordBasedSuggestions": false,
                "editor.suggest.snippetsPreventQuickSuggestions": false
            },
            "[toml]": {
                "editor.wordBasedSuggestions": false
            }
        },
        "breakpoints": [
            {
                "language": "ballerina"
            }
        ],
        "debuggers": [
            {
                "type": "ballerina",
                "label": "KolaB Debug",
                "languages": [
                    "ballerina"
                ],
                "program": "/usr/local/Cellar/ballerina/0.991.0/libexec/lib/tools/debug-adapter/launcher/debug-adapter-launcher.sh",
                "configurationAttributes": {
                    "launch": {
                        "required": [],
                        "properties": {
                            "script": {
                                "type": "string",
                                "default": "${file}",
                                "description": "Path of the script that need to be executed (e.g. ${workspaceRoot}/main.bal)"
                            },
                            "programArgs": [],
                            "commandOptions": [],
                            "env": {},
                            "debugServerTimeout": {
                                "type": "integer",
                                "default": 5000,
                                "description": "How long to wait for the debug server to start up in milliseconds"
                            },
                            "debugTests": {
                                "type": "boolean",
                                "default": false,
                                "description": "Indicates whether to debug the tests for the given script."
                            },
                            "networkLogs": {
                                "type": "boolean",
                                "default": false,
                                "description": "Indicates whether to capture network logs."
                            },
                            "terminal": {
                                "type": "string",
                                "enum": [
                                    "integrated",
                                    "external"
                                ],
                                "enumDescriptions": [
                                    "Launch program in an integrated VSCode terminal.",
                                    "Launch program in an external terminal."
                                ],
                                "default": "integrated",
                                "description": "Indicates the terminal kind to launch the debugging process in."
                            }
                        }
                    },
                    "attach": {
                        "required": [
                            "debuggeePort",
                            "debuggeeHost"
                        ],
                        "properties": {
                            "debuggeeHost": {
                                "type": "string",
                                "description": "Remote debug server host",
                                "default": "127.0.0.1"
                            },
                            "debuggeePort": {
                                "type": "string",
                                "description": "Remote debug server port",
                                "default": "5005"
                            }
                        }
                    }
                },
                "initialConfigurations": [
                    {
                        "name": "Ballerina Debug",
                        "type": "ballerina",
                        "request": "launch",
                        "programArgs": [],
                        "commandOptions": [],
                        "env": {}
                    },
                    {
                        "name": "Ballerina Test",
                        "type": "ballerina",
                        "request": "launch",
                        "debugTests": true,
                        "programArgs": [],
                        "commandOptions": [],
                        "env": {}
                    },
                    {
                        "name": "Ballerina Remote",
                        "type": "ballerina",
                        "request": "attach",
                        "debuggeeHost": "127.0.0.1",
                        "debuggeePort": "5005"
                    }
                ]
            }
        ],
        "commands": [
            {
                "command": "ballerina.installBallerina",
                "title": "Install Ballerina",
                "category": "Ballerina"
            },
            {
                "command": "ballerina.setupKola",
                "title": "Set up Kola",
                "category": "Ballerina"
            },
            {
                "command": "ballerina.updateKola",
                "title": "Update Kola",
                "category": "Ballerina"
            },
            {
                "command": "kolab.showExamples",
                "title": "Show Examples",
                "category": "Ballerina"
            },
            {
                "command": "kolab.project.test",
                "title": "Test",
                "category": "Ballerina"
            },
            {
                "command": "kolab.project.build",
                "title": "Build",
                "category": "Ballerina"
            },
            {
                "command": "kolab.project.pack",
                "title": "Pack",
                "category": "Ballerina"
            },
            {
                "command": "kolab.create.cloud",
                "title": "Create Cloud.toml",
                "category": "Ballerina"
            },
            {
                "command": "kolab.project.doc",
                "title": "Build Documentation",
                "category": "Ballerina"
            },
            {
                "command": "kolab.project.add",
                "title": "Add Module",
                "category": "Ballerina"
            },
            {
                "command": "kolab.new.module",
                "title": "Add Module",
                "category": "Ballerina",
                "icon": "$(distro-new-module)"
            },
            {
                "command": "kolab.refreshPackageTree",
                "title": "Refresh",
                "icon": "$(distro-refresh)"
            },
            {
                "command": "kolab.refreshExplorerTree",
                "title": "Refresh",
                "icon": "$(distro-refresh)"
            },
            {
                "command": "kolab.newFileExplorerTree",
                "title": "New File",
                "icon": "$(distro-new-file)"
            },
            {
                "command": "kolab.newFolderExplorerTree",
                "title": "New Folder",
                "icon": "$(distro-new-folder)"
            },
            {
                "command": "kolab.deleteFileExplorerTree",
                "title": "Delete",
                "icon": "$(distro-delete)"
            },
            {
                "command": "kolab.show.visualizer",
                "title": "Show Visualizer",
                "icon": "$(distro-design-view)",
                "category": "Ballerina"
            },
            {
                "command": "kolab.project.run",
                "title": "Run",
                "icon": {
                    "dark": "./resources/images/icons/start-inverse.svg",
                    "light": "./resources/images/icons/start.svg"
                },
                "category": "Ballerina"
            },
            {
                "command": "kolab.project.run.config",
                "title": "Run",
                "icon": {
                    "dark": "./resources/images/icons/start-inverse.svg",
                    "light": "./resources/images/icons/start.svg"
                }
            },
            {
                "command": "kolab.project.stop",
                "title": "Stop",
                "category": "Ballerina"
            },
            {
                "command": "kolab.source.debug",
                "title": "Debug",
                "icon": "$(distro-debug)",
                "category": "Ballerina"
            },
            {
                "command": "kolab.show.source",
                "title": "Show Source",
                "icon": "$(code)",
                "category": "Ballerina"
            },
            {
                "command": "kolab.pasteAsRecord",
                "title": "Paste JSON as Record",
                "category": "Ballerina"
            },
            {
                "command": "kolab.pasteXMLAsRecord",
                "title": "Paste XML as Record",
                "category": "Ballerina"
            },
            {
                "command": "kolab.project.config.create",
                "title": "Create Config.toml",
                "category": "Ballerina"
            },
            {
                "command": "kolab.notebook.restartNotebook",
                "title": "Restart Notebook",
                "icon": "$(distro-refresh)",
                "category": "Ballerina",
                "when": "notebookType == 'kolab-notebook'"
            },
            {
                "command": "kolab.notebook.openVariableView",
                "title": "Notebook Variables",
                "icon": "$(notebook)",
                "category": "Ballerina",
                "enablement": "notebookType == 'kolab-notebook'"
            },
            {
                "command": "kolab.notebook.openOutlineView",
                "title": "Notebook Outline",
                "icon": "$(list-unordered)",
                "category": "Ballerina",
                "enablement": "notebookType == 'kolab-notebook'"
            },
            {
                "command": "kolab.notebook.debug",
                "title": "Debug Cell",
                "category": "Ballerina",
                "enablement": "isNotebookDebugModeEnabled"
            },
            {
                "command": "kolab.notebook.refreshVariableView",
                "title": "Refresh",
                "icon": "$(distro-refresh)",
                "category": "Ballerina",
                "enablement": "isNotebookVariableViewEnabled"
            },
            {
                "command": "kolab.notebook.createNotebook",
                "title": "Create Notebook",
                "category": "Ballerina",
                "icon": "$(notebook)"
            },
            {
                "command": "kolab.view.architectureView",
                "title": "Architecture View",
                "category": "Ballerina"
            },
            {
                "command": "kolab.view.serviceDesigner",
                "title": "Service Designer View",
                "category": "Ballerina"
            },
            {
                "command": "kolab.view.cellView",
                "title": "Cell View",
                "category": "Ballerina"
            },
            {
                "command": "kolab.view.entityDiagram",
                "title": "Entity Relationship Diagram",
                "icon": "$(distro-persist-diagram)",
                "category": "Ballerina"
            },
            {
                "command": "kolab.open.ai.panel",
                "title": "Open AI Panel",
                "category": "Ballerina",
                "icon": "$(wand)"
            },
            {
                "command": "kolab.open.bi.welcome",
                "title": "Open Welcome",
                "category": "BI"
            },
            {
                "command": "BI.project.run",
                "title": "Run",
                "icon": {
                    "dark": "./resources/images/icons/start-inverse.svg",
                    "light": "./resources/images/icons/start.svg"
                },
                "category": "BI"
            },
            {
                "command": "BI.project-explorer.refresh",
                "title": "Refresh Kola",
                "category": "BI"
            },
            {
                "command": "BI.view.typeDiagram",
                "title": "Type Diagram",
                "category": "BI"
            },
            {
                "command": "BI.project-explorer.add-connection",
                "title": "Add Connection",
                "icon": "$(add)",
                "group": "navigation",
                "category": "BI"
            },
            {
                "command": "BI.project-explorer.add-entry-point",
                "title": "Add Entry Point",
                "icon": "$(add)",
                "group": "navigation",
                "category": "BI"
            },
            {
                "command": "BI.project-explorer.add-type",
                "title": "Add Types",
                "icon": "$(add)",
                "group": "navigation",
                "category": "BI"
            },
            {
                "command": "BI.project-explorer.add-function",
                "title": "Add Function",
                "icon": "$(add)",
                "group": "navigation",
                "category": "BI"
            },
            {
                "command": "BI.project-explorer.add-configuration",
                "title": "Add Configuration",
                "icon": "$(add)",
                "group": "navigation",
                "category": "BI"
            }
        ],
        "views": {
            "notebook-variables": [
                {
                    "type": "webview",
                    "id": "ballerinaViewVariables",
                    "name": "Variable View",
                    "when": "isNotebookVariableViewEnabled"
                }
            ]
        },
        "viewsContainers": {
            "panel": [
                {
                    "id": "notebook-variables",
                    "title": "Ballerina Notebook",
                    "icon": "$(notebook)"
                }
            ]
        },
        "menus": {
            "view/title": [
                {
                    "command": "kolab.notebook.refreshVariableView",
                    "when": "view == ballerinaViewVariables",
                    "group": "navigation"
                }
            ],
            "editor/title": [
                {
                    "command": "kolab.project.run",
                    "group": "navigation@1",
                    "title": "Run project in terminal",
                    "when": "resourceLangId == ballerina || isBallerinaDiagram"
                },
                {
                    "command": "kolab.project.run.config",
                    "group": "navigation@1",
                    "title": "Run project in terminal",
                    "when": "resourceLangId == toml && isBallerinaProject"
                },
                {
                    "command": "kolab.source.debug",
                    "group": "navigation@2",
                    "title": "Debug project in terminal",
                    "when": "resourceLangId == ballerina"
                },
                {
                    "when": "resourceLangId == ballerina && !isPersistModelActive",
                    "command": "kolab.show.visualizer",
                    "group": "navigation@3"
                },
                {
                    "when": "resourceLangId == ballerina && isPersistModelActive",
                    "command": "kolab.view.entityDiagram",
                    "group": "navigation@3"
                },
                {
                    "when": "isBallerinaDiagram",
                    "command": "kolab.show.source",
                    "group": "navigation@4"
                },
                {
                    "when": "isBalVisualizerActive && showBalGoToSource",
                    "command": "kolab.show.source",
                    "group": "navigation@3",
                    "title": "Show Source"
                },
                {
                    "command": "kolab.open.ai.panel",
                    "group": "navigation@1",
                    "when": "resourceLangId == ballerina || isBallerinaDiagram || (isBalVisualizerActive && isBIProject)",
                    "title": "Open AI Panel"
                },
                {
                    "command": "BI.project.run",
                    "group": "navigation@1",
                    "title": "Run project",
                    "when": "isBalVisualizerActive && isBIProject"
                }
            ],
            "notebook/toolbar": [
                {
                    "command": "kolab.notebook.restartNotebook",
                    "title": "Restart Notebook",
                    "group": "navigation@1",
                    "when": "notebookType == 'kolab-notebook'"
                },
                {
                    "command": "kolab.notebook.openVariableView",
                    "title": "Variables",
                    "group": "navigation@2",
                    "when": "notebookType == 'kolab-notebook'"
                },
                {
                    "command": "kolab.notebook.openOutlineView",
                    "title": "Outline",
                    "group": "navigation@3",
                    "when": "notebookType == 'kolab-notebook'"
                }
            ],
            "notebook/cell/execute": [
                {
                    "command": "kolab.notebook.debug",
                    "title": "Debug",
                    "group": "navigation@1",
                    "when": "notebookType == 'kolab-notebook' && isNotebookDebugModeEnabled"
                }
            ],
            "explorer/context": [
                {
                    "command": "kolab.project.run",
                    "group": "Ballerina",
                    "when": "resourceLangId == ballerina"
                }
            ],
            "commandPalette": [
                {
                    "command": "kolab.new.module",
                    "when": "editorLangId != ballerina && !isBallerinaDiagram"
                },
                {
                    "command": "kolab.show.source",
                    "when": "editorLangId != ballerina && !isBallerinaDiagram"
                },
                {
                    "command": "kolab.show.visualizer",
                    "when": "editorLangId == ballerina"
                },
                {
                    "command": "kolab.source.debug",
                    "when": "editorLangId != ballerina && !isBallerinaDiagram"
                },
                {
                    "command": "kolab.notebook.createNotebook"
                },
                {
                    "command": "kolab.view.entityDiagram",
                    "when": "isPersistModelActive"
                }
            ]
        },
        "notebooks": [
            {
                "id": "kolab-notebook",
                "type": "kolab-notebook",
                "displayName": "Ballerina Notebook",
                "selector": [
                    {
                        "filenamePattern": "*.balnotebook"
                    }
                ]
            }
        ],
        "notebookRenderer": [
            {
                "id": "kolab-notebook-renderer",
                "displayName": "Ballerina Notebook Renderer",
                "entrypoint": "resources/jslibs/NotebookRenderer.js",
                "mimeTypes": [
                    "kolab-notebook/json-view",
                    "kolab-notebook/table-view",
                    "kolab-notebook/xml-view"
                ]
            }
        ],
        "icons": {
            "distro-new-module": {
                "description": "new-module",
                "default": {
                    "fontPath": "./resources/font-wso2-vscode/dist/wso2-vscode.woff",
                    "fontCharacter": "\\f135"
                }
            },
            "distro-refresh": {
                "description": "refresh",
                "default": {
                    "fontPath": "./resources/codicons/codicon.ttf",
                    "fontCharacter": "\\eb37"
                }
            },
            "distro-new-file": {
                "description": "new-file",
                "default": {
                    "fontPath": "./resources/codicons/codicon.ttf",
                    "fontCharacter": "\\ea7f"
                }
            },
            "distro-new-folder": {
                "description": "new-folder",
                "default": {
                    "fontPath": "./resources/codicons/codicon.ttf",
                    "fontCharacter": "\\ea80"
                }
            },
            "distro-delete": {
                "description": "delete",
                "default": {
                    "fontPath": "./resources/font-wso2-vscode/dist/wso2-vscode.woff",
                    "fontCharacter": "\\f114"
                }
            },
            "distro-design-view": {
                "description": "design-view",
                "default": {
                    "fontPath": "./resources/font-wso2-vscode/dist/wso2-vscode.woff",
                    "fontCharacter": "\\f139"
                }
            },
            "distro-start": {
                "description": "start",
                "default": {
                    "fontPath": "./resources/font-wso2-vscode/dist/wso2-vscode.woff",
                    "fontCharacter": "\\f1a8"
                }
            },
            "distro-debug": {
                "description": "debug",
                "default": {
                    "fontPath": "./resources/font-wso2-vscode/dist/wso2-vscode.woff",
                    "fontCharacter": "\\f134"
                }
            },
            "distro-source-view": {
                "description": "source-view",
                "default": {
                    "fontPath": "./resources/font-wso2-vscode/dist/wso2-vscode.woff",
                    "fontCharacter": "\\f149"
                }
            },
            "distro-persist-diagram": {
                "description": "persist-diagram",
                "default": {
                    "fontPath": "./resources/font-wso2-vscode/dist/wso2-vscode.woff",
                    "fontCharacter": "\\f185"
                }
            },
            "distro-cached-rounded": {
                "description": "cached-rounded",
                "default": {
                    "fontPath": "./resources/font-wso2-vscode/dist/wso2-vscode.woff",
                    "fontCharacter": "\\f106"
                }
            },
            "distro-cached-round": {
                "description": "cached-round",
                "default": {
                    "fontPath": "./resources/font-wso2-vscode/dist/wso2-vscode.woff",
                    "fontCharacter": "\\f107"
                }
            },
            "distro-fit-to-screen": {
                "description": "fit-to-screen",
                "default": {
                    "fontPath": "./resources/font-wso2-vscode/dist/wso2-vscode.woff",
                    "fontCharacter": "\\f120"
                }
            },
            "distro-explicit-outlined": {
                "description": "explicit-outlined",

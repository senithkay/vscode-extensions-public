  "typings": "build/index",
  "scripts": {
    "build": "pnpm run lint && pnpm run storybook:setup && pnpm run build:tsc",
    "build:tsc": "tsc --pretty && pnpm run copy:assets",
    "build:webpack": "webpack --config webpack.config.js --mode=production",
    "copy:assets": "copyfiles -u 1 \"src/**/*.scss\" \"src/**/*.svg\"  \"src/**/*.css\" \"src/**/*.png\" \"src/**/*.txt\" \"src/**/*.json\" \"src/assets/fonts/Glimer/*.*\" build/",
    "clean": "rimraf build/",
    "clean:nm": "rimraf node_modules",
    "lint": "tslint --project .",
    "lint:fix": "tslint --fix --project .",
    "stylelint": "stylelint 'src/**/*.scss' --config ./.stylelintrc.json --syntax scss --fix",
    "watch": "pnpm run storybook:setup && pnpm run watch:webpack",
    "watch:tsc": "tsc --pretty --watch",
    "watch:webpack": "webpack-dev-server --mode=development --progress",
    "deploy": "pnpm publish",
    "storybook": "pnpm run storybook:setup && node tools/start-storybook.js",
    "storybook:setup": "node tools/setup-storybook.js",
    "cypress": "cypress open",
    "build-storybook": "build-storybook"
  },

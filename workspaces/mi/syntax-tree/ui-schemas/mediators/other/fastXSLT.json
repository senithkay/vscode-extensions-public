{
  "name": "Fast XSLT",
  "type": "mediator",
  "title": "Fast XSLT Mediator",
  "help": "Swiftly transforms message streams (not payloads) based on an XSLT script.",
  "elements": [
    {
      "type": "attributeGroup",
      "value": {
        "groupName": "SchemaKey",
        "elements": [
          {
            "type": "attribute",
            "value": {
              "name": "fastXsltSchemaType",
              "displayName": "Fast Xslt Schema Type",
              "inputType": "combo",
              "defaultValue": "Static",
              "comboValues": [
                "Static",
                "Dynamic"
              ],
              "required": false,
              "helpTip": ""
            }
          },
          {
            "type": "attribute",
            "value": {
              "name": "fastXsltDynamicSchemaKey",
              "displayName": "Fast Xslt Dynamic SchemaKey",
              "inputType": "expression",
              "defaultValue": "",
              "required": true,
              "helpTip": "",
              "enableCondition": [
                {
                  "fastXsltSchemaType": "Dynamic"
                }
              ]
            }
          },
          {
            "type": "attribute",
            "value": {
              "name": "fastXsltStaticSchemaKey",
              "displayName": "Fast Xslt Static SchemaKey",
              "inputType": "key",
              "keyType": "sequence",
              "defaultValue": "",
              "required": true,
              "helpTip": "",
              "enableCondition": [
                {
                  "fastXsltSchemaType": "Static"
                }
              ]
            }
          }
        ]
      }
    },
    {
      "type": "attributeGroup",
      "value": {
        "groupName": "Misc",
        "elements": [
          {
            "type": "attribute",
            "value": {
              "name": "description",
              "displayName": "Description",
              "inputType": "string",
              "defaultValue": "",
              "required": false,
              "helpTip": ""
            }
          }
        ]
      }
    }
  ]
}

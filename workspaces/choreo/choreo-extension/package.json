{
  "name": "choreo",
  "displayName": "Choreo",
  "description": "An extension for Signing in to Choreo and managing your projects",
  "version": "2.0.0",
  "cliVersion": "v1.0.472404291208",
  "publisher": "wso2",
  "bugs": {
    "url": "https://github.com/wso2/choreo-vscode/issues"
  },
  "icon": "resources/icons/choreo.png",
  "engines": {
    "vscode": "^1.83.1"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "extensionDependencies": [
    "redhat.vscode-yaml"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "yaml",
        "aliases": [
          "Component Configuration"
        ],
        "extensions": [
          ".yaml"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "commands": [
      {
        "command": "wso2.choreo.project.clone",
        "title": "Clone to new Workspace"
      },
      {
        "command": "wso2.choreo.project.repo.clone",
        "title": "Clone a repo to current Project Workspace"
      },
      {
        "command": "wso2.choreo.project.open",
        "title": "Open Choreo Project",
        "category": "Choreo",
        "icon": "$(link-external)"
      },
      {
        "command": "wso2.choreo.project.delete",
        "title": "Delete Choreo Project",
        "category": "Choreo",
        "icon": "$(trash)"
      },
      {
        "command": "wso2.choreo.cell.view",
        "title": "View Choreo Cell Diagram",
        "category": "Choreo"
      },

      {
        "command": "wso2.choreo.getStarted",
        "title": "Get Started",
        "category": "Choreo"
      },
      {
        "command": "wso2.choreo.sign.in",
        "title": "Sign In",
        "category": "Choreo",
        "icon": "${sign-in}",
        "enablement": "isLoggedIn == false"
      },
      {
        "command": "wso2.choreo.sign.in.with.authCode",
        "title": "Sign In with Auth Code",
        "category": "Choreo",
        "enablement": "isLoggedIn == false"
      },
      {
        "command": "wso2.choreo.sign.out",
        "title": "Sign Out",
        "category": "Choreo",
        "icon": "${sign-out}",
        "enablement": "isLoggedIn == true"
      },
      {
        "command": "wso2.choreo.create.component",
        "title": "Create Component",
        "category": "Choreo",
        "icon": "$(add)",
        "enablement": "isValidWorkspace == true"
      },
      {
        "command": "wso2.choreo.refresh.component",
        "title": "Refresh Components",
        "category": "Choreo",
        "icon": "$(refresh)",
        "enablement": "isLoggedIn == true && isValidWorkspace == true && isLoadingLinkedDirs == false"
      },
      {
        "command": "wso2.choreo.link.component",
        "title": "Link Directory",
        "category": "Choreo",
        "icon": "$(link)",
        "enablement": "isValidWorkspace == true"
      },
      {
        "command": "wso2.choreo.component.open.external",
        "title": "Open in Console",
        "category": "Choreo",
        "icon": "$(link-external)"
      },
      {
        "command": "wso2.choreo.unlink.component",
        "title": "Unlink Directory",
        "category": "Choreo",
        "icon": "$(debug-disconnect)",
        "enablement": "isValidWorkspace == true"
      },
      {
        "command": "wso2.choreo.delete.component",
        "title": "Delete Component",
        "category": "Choreo",
        "icon": "$(trash)"
      },
      {
        "command": "wso2.choreo.component.view",
        "title": "View Component Details",
        "category": "Choreo",
        "icon": "$(preview)"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "wso2-choreo",
          "title": "Choreo",
          "icon": "$(distro-choreo-2)"
        }
      ]
    },
    "views": {
      "wso2-choreo": [
        {
          "id": "choreo.activity.components",
          "name": "Components",
          "type": "webview"
        },
        {
          "id": "choreo.activity.account",
          "name": "Account",
          "type": "webview"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "wso2.choreo.link.component",
          "when": "view == choreo.activity.components && isLoggedIn == true",
          "group": "navigation"
        },
        {
          "command": "wso2.choreo.create.component",
          "when": "view == choreo.activity.components && isLoggedIn == true",
          "group": "navigation"
        },
        {
          "command": "wso2.choreo.refresh.component",
          "when": "view == choreo.activity.components && isLoggedIn == true",
          "group": "navigation"
        }
      ],
      "webview/context": [
        {
          "command": "wso2.choreo.component.open.external",
          "when": "webviewId == choreo.activity.components && webviewSection == 'validLinkItem'",
          "group": "access"
        },
        {
          "command": "wso2.choreo.unlink.component",
          "when": "(webviewSection == 'validLinkItem' || webviewSection == 'invalidLinkItem')",
          "group": "destructive"
        },
        {
          "command": "wso2.choreo.delete.component",
          "when": "webviewSection == 'validLinkItem'",
          "group": "destructive"
        }
      ]
    },
    "configuration": {
      "type": "object",
      "title": "Choreo",
      "properties": {
        "Logging.loggingLevel": {
          "type": "string",
          "enum": [
            "off",
            "fatal",
            "error",
            "warn",
            "info",
            "debug",
            "trace"
          ],
          "default": "error",
          "description": "The verbosity of logging. The Order is off < fatal < error < warn < info < debug < trace.",
          "order": 1,
          "scope": "window"
        },
        "Logging.sourceLocationTracking": {
          "type": "boolean",
          "default": false,
          "description": "Should Source Code Location Info be added to log entries, DANGER - May be very slow, only use in debugging scenarios",
          "order": 1,
          "scope": "window"
        },
        "Advanced.ChoreoEnvironment": {
          "type": "string",
          "enum": [
            "prod",
            "stage",
            "dev"
          ],
          "default": "prod",
          "description": "The Choreo Enviornment to use",
          "scope": "window"
        },
        "Advanced.RpcPath": {
          "type": "string",
          "default": "",
          "description": "The path to Choreo RPC server",
          "scope": "window"
        }
      }
    },
    "walkthroughs": [
      {
        "id": "choreo.getStarted",
        "title": "Getting started with Choreo",
        "description": "Utilize the Choreo VSCode extension to enhance and streamline your developer experience with Choreo components. Easily access and manage components associated with your currently opened VSCode workspace through the Choreo panel on the left activity bar.",
        "steps": [
          {
            "id": "createComponent",
            "title": "Create components",
            "description": "Choreo simplifies the deployment and management of your source code in the cloud through components. \nVarious types of components are supported, including Services, Manual Triggers, Cron Jobs, Web Applications, and more.\n[Create Component](command:wso2.choreo.create.component)\n Once created, you will be able to easily access your components from the [Choreo activity panel](command:choreo.activity.components.focus).",
            "media": {
              "markdown": "resources/walkthrough/comp-create.md"
            }
          },
          {
            "id": "componentLinks",
            "title": "Component links",
            "description": "Upon component creation, a .choreo/link.yaml will be created within the component directory, generating an association between the directory and the created component. \nIf the link.yaml file is not available, you can generate it by linking the directory with the component.\n[Link Directory](command:wso2.choreo.link.component)",
            "media": {
              "markdown": "resources/walkthrough/link-dir.md"
            }
          },
          {
            "id": "manageComponent",
            "title": "Manage your components",
            "description": "Access the component details view to manage a selected component. This view is accessible by selecting a component in the [Choreo activity panel](command:choreo.activity.components.focus) or by running the [View Component Details](command:wso2.choreo.component.view) command.\n**Build component**: Initiate a new build by selecting the commit you wish to build. In case of build failures, diagnose them by reviewing the build logs.\n**Deploy component**: Once a component's commit has been successfully built, deploy the build onto any available environment (e.g., Development, Production, etc.).\n**View Logs**: After successful deployment, monitor real-time application and gateway logs of the component within VSCode's integrated terminal.",
            "media": {
              "markdown": "resources/walkthrough/comp-details.md"
            }
          }
        ]
      }
    ],
    "icons": {
      "distro-choreo-2": {
        "description": "choreo-2",
        "default": {
          "fontPath": "./resources/font-wso2-vscode/dist/wso2-vscode.woff",
          "fontCharacter": "\\f10a"
        }
      }
    }
  },
  "scripts": {
    "clean": "del-cli ./dist ./out ./resources/jslibs ./choreo-*.vsix ./coverage ./.nyc_output",
    "compile": "webpack --mode production",
    "watch-choreo": "npx webpack --watch",
    "build": "pnpm run compile && pnpm run lint && pnpm run copy-webview-lib && pnpm copy-cellview-lib && pnpm run postbuild",
    "rebuild": "pnpm run clean && pnpm run compile && pnpm run lint && pnpm run copy-webview-lib && pnpm copy-cellview-lib && pnpm run postbuild",
    "copy-webview-lib": "mkdir -p ./resources/jslibs && copyfiles -f ../choreo-webviews/build/* ./resources/jslibs",
    "copy-cellview-lib": "mkdir -p ./resources/jslibs && copyfiles -u 3 \"../cell-diagram/lib/**/*\" ./resources/jslibs",
    "compile-tests": "tsc -p . --outDir dist",
    "watch-tests": "tsc -p . -w --outDir dist",
    "e2e-test-setup": "npx extest get-vscode -c 1.83.1 && npx extest get-chromedriver -c 1.83.1 && npx extest install-vsix -f $(ls vsix/*.vsix) && npx extest install-vsix -f $(ls ../../../ballerina-*.vsix)",
    "e2e-test": "pnpm run compile-tests && npx extest run-tests 'dist/src/tests/e2e-tests/*.test.js' --mocha_config src/tests/e2e-tests/.mocharc.js -o src/tests/e2e-tests/settings.json",
    "e2e-playwright": "pnpm run compile-tests && pnpm exec playwright test",
    "e2e-pw-record": "pnpm run compile-tests && node ./dist/tests/e2e-playwright/recorder.js",
    "integration-test": "pnpm run compile-tests && npx extest run-tests 'dist/tests/integration-tests/*.test.js' --mocha_config src/tests/integration-tests/.mocharc.js -o src/tests/integration-tests/settings.json",
    "pretest": "pnpm run clean && pnpm run compile-tests && pnpm run lint && pnpm run copy-webview-lib && pnpm run copy-cellview-lib",
    "lint": "eslint src --ext ts",
    "test-nyc-cli": "nyc --reporter=html node ./dist/test/runTest.js",
    "test": "rm -rf .vscode-test/extensions && node ./dist/test/runTest.js && nyc report && nyc report --reporter=html",
    "package": "if [ $isPreRelease = true ]; then vsce package --no-dependencies --pre-release --baseImagesUrl https://github.com/wso2/choreo-vscode/raw/HEAD/; else vsce package --no-dependencies --baseImagesUrl https://github.com/wso2/choreo-vscode/raw/HEAD/; fi",
    "copyFonts": "copyfiles -f ./node_modules/@wso2-enterprise/font-wso2-vscode/dist/* ./resources/font-wso2-vscode/dist/",
    "copyVSIX": "copyfiles *.vsix ./vsix",
    "copyVSIXToRoot": "copyfiles -f ./vsix/* ../../..",
    "postbuild": "pnpm -r run postbuild && pnpm run copyFonts && pnpm run package && pnpm run copyVSIX"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@playwright/test": "~1.37.0",
    "@types/byline": "^4.2.33",
    "@types/glob": "^8.0.0",
    "@types/js-yaml": "^4.0.5",
    "@types/mocha": "~10.0.1",
    "@types/nock": "~11.1.0",
    "@types/node": "^18.18.2",
    "@types/sinon": "^10.0.0",
    "@types/vscode": "^1.73.0",
    "@types/which": "^2.0.2",
    "@typescript-eslint/eslint-plugin": "~5.48.2",
    "@typescript-eslint/parser": "~5.48.2",
    "@vscode/test-electron": "^2.2.0",
    "@vscode/vsce": "^2.17.0",
    "@wso2-enterprise/playwright-vscode-tester": "workspace:*",
    "axios": "^1.2.2",
    "copy-webpack-plugin": "^11.0.0",
    "copyfiles": "^2.4.1",
    "del-cli": "^5.0.0",
    "eslint": "^8.32.0",
    "glob": "^7.2.0",
    "mocha": "^10.2.0",
    "nock": "~13.3.2",
    "nyc": "^15.1.0",
    "sinon": "^15.0.1",
    "source-map-support": "^0.5.21",
    "ts-loader": "^9.4.1",
    "typescript": "^4.9.4",
    "vscode-extension-tester": "^5.9.1",
    "webpack": "^5.56.0",
    "webpack-cli": "^4.10.0",
    "webpack-permissions-plugin": "^1.0.9",
    "yarn": "^1.22.19"
  },
  "dependencies": {
    "@types/chai": "^4.2.18",
    "@types/lodash": "^4.14.189",
    "@vscode-logging/logger": "^1.2.3",
    "@vscode-logging/types": "^0.1.4",
    "@vscode-logging/wrapper": "^1.0.2",
    "@vscode/extension-telemetry": "~0.7.7",
    "@vscode/iconv-lite-umd": "^0.7.0",
    "@vscode/webview-ui-toolkit": "^1.2.0",
    "@wso2-enterprise/ballerina-languageclient": "workspace:*",
    "@wso2-enterprise/choreo-client": "workspace:*",
    "@wso2-enterprise/choreo-core": "workspace:*",
    "@wso2-enterprise/choreo-vscode-webviews": "workspace:*",
    "@wso2-enterprise/font-wso2-vscode": "workspace:*",
    "byline": "^5.0.0",
    "chai": "^4.3.4",
    "dotenv": "^16.0.3",
    "file-type": "^18.2.1",
    "gql-query-builder": "^3.8.0",
    "graphql": "^16.6.0",
    "graphql-request": "~5.1.0",
    "js-yaml": "^4.1.0",
    "jschardet": "^3.0.0",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.11",
    "simple-git": "^3.15.1",
    "vscode-messenger": "^0.4.4",
    "vscode-messenger-common": "^0.4.3",
    "which": "^3.0.0",
    "vscode-jsonrpc": "^6.0.0",
    "vscode-languageserver-protocol": "^3.16.0",
    "@wso2-enterprise/cell-diagram": "workspace:*",
    "zustand": "^4.5.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wso2-enterprise/ballerina-plugin-vscode.git"
  }
}

{
  "name": "@wso2-enterprise/ballerina-configurable-editor",
  "version": "3.3.14-beta.1",
  "description": "Ballerina Configurable Editor",
  "files": [
    "build"
  ],
  "main": "build/esm/index.js",
  "types": "build/esm/index.d.ts",
  "typings": "build/esm/index",
  "scripts": {
    "prebuild": "pnpm run lint",
    "build": "pnpm run build:webpack && tsc --pretty && pnpm run copy:assets && pnpm run postbuild",
    "build:tsc": "tsc --pretty && pnpm run copy:assets",
    "build:webpack": "webpack --config webpack.config.js --mode=production",
    "copy:assets": "copyfiles -u 1 src/**/*.scss src/**/*.svg src/**/*.css src/**/*.png src/**/*.txt src/**/*.json build/esm",
    "watch": "pnpm run watch:webpack",
    "watch:tsc": "tsc --pretty --watch",
    "watch:webpack": "webpack-dev-server --mode=development --progress",
    "clean": "rimraf build/",
    "clean:nm": "rimraf node_modules",
    "lint": "tslint --project .",
    "lint:fix": "tslint --fix --project .",
    "stylelint": "stylelint 'src/**/*.scss' --config ./.stylelintrc.json --syntax scss --fix",
    "deploy": "pnpm publish",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test": "react-scripts test --watchAll=false",
    "postbuild": "copyfiles -f build/umd/ConfigEditor.js ../ballerina-extension/resources/jslibs"
  },
  "dependencies": {
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.9.1",
    "@types/node": "^18.11.19",
    "tslint": "^6.1.3",
    "ts-loader": "^9.4.1",
    "svg-url-loader": "^8.0.0",
    "@types/jest": "^29.2.5",
    "copyfiles": "^2.4.1",
    "clsx": "^1.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@material-ui/lab": "^4.0.0-alpha.52",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/builder-webpack5": "^6.5.9",
    "@storybook/cli": "^6.4.1",
    "@storybook/manager-webpack5": "^6.5.9",
    "@storybook/react": "^6.5.9",
    "@storybook/testing-react": "^1.3.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.5",
    "@types/react": "^17.0.37",
    "@types/react-dom": "17.0.14",
    "babel-loader": "^8.2.5",
    "css-loader": "^5.2.7",
    "react-scripts": "^5.0.1",
    "source-map-loader": "^4.0.1",
    "style-loader": "^1.3.0",
    "typescript": "^4.9.4",
    "webpack": "^5.56.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "@types/node": "^18.11.19",
    "ts-loader": "^9.4.1",
    "tslint": "^6.1.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wso2-enterprise/ballerina-plugin-vscode.git"
  },
  "author": "wso2",
  "license": "UNLICENSED",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "resolutions": {
    "@storybook/react/webpack": "^5.56.0",
    "@storybook/core-common/webpack": "^5.56.0",
    "@storybook/core-server/webpack": "^5.56.0",
    "@storybook/builder-webpack4/webpack": "^5.56.0",
    "@storybook/manager-webpack4/webpack": "^5.56.0"
  }
}

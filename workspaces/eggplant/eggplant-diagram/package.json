{
    "name": "@wso2-enterprise/eggplant-diagram",
    "version": "1.0.0",
    "description": "New low code diagram for a Ballerina project",
    "files": [
        "lib"
    ],
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "scripts": {
        "watch": "tsc --pretty --watch",
        "build": "tsc --pretty && npm run copy:assets && pnpm run postbuild",
        "storybook": "start-storybook -p 6006",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "copy:assets": "copyfiles -u 1 \"src/**/*.svg\"  \"src/**/*.css\" \"src/resources/assets/font/*.*\" lib/",
        "postbuild": "copyfiles -u 1 -V build/*.js build/fonts/* build/images/* -e build/*.txt ../ballerina-extension/resources/jslibs"
    },
    "keywords": [],
    "dependencies": {
        "@emotion/react": "^11.9.3",
        "@emotion/styled": "^11.10.5",
        "@material-ui/core": "^4.12.3",
        "@mui/material": "~5.13.2",
        "@mui/icons-material": "^5.10.3",
        "@projectstorm/geometry": "^6.7.4",
        "@projectstorm/react-canvas-core": "^6.7.4",
        "@projectstorm/react-diagrams": "^6.7.4",
        "@projectstorm/react-diagrams-core": "^6.7.4",
        "@projectstorm/react-diagrams-defaults": "^6.7.4",
        "@projectstorm/react-diagrams-routing": "^6.7.4",
        "@types/lodash": "^4.14.189",
        "@types/node": "^18.11.19",
        "closest": "^0.0.1",
        "dagre": "^0.8.5",
        "file-loader": "^5.0.2",
        "html-to-image": "^1.10.8",
        "lodash": "^4.17.11",
        "pathfinding": "^0.4.18",
        "paths-js": "^0.4.11",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "resize-observer-polyfill": "^1.5.1",
        "typescript": "^4.9.4",
        "@wso2-enterprise/ui-toolkit": "workspace:*"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^6.9.1",
        "@typescript-eslint/parser": "^6.9.1",
        "@babel/core": "^7.20.12",
        "@storybook/addon-actions": "^6.5.9",
        "@storybook/addon-essentials": "^6.5.9",
        "@storybook/addon-interactions": "^6.5.9",
        "@storybook/addon-links": "^6.5.9",
        "@storybook/builder-webpack5": "^6.5.9",
        "@storybook/manager-webpack5": "^6.5.9",

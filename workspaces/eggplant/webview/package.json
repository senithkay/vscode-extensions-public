{
  "name": "@wso2-enterprise/eggplant-webview",
  "version": "1.0.0",
  "description": "Eggplant Extension Visualizer Webview",
  "files": [
    "lib"
  ],
  "main": "lib/DesignDiagram.js",
  "types": "lib/DesignDiagram.d.ts",
  "scripts": {
    "start": "webpack-dev-server --progress --mode=development",
    "build": "tsc --pretty && npm run copy:assets && webpack && pnpm run postbuild",
    "copy:assets": "copyfiles -u 1 \"src/**/*.svg\"  \"src/**/*.css\" \"src/resources/assets/font/*.*\" lib/",
    "deploy": "npm publish",
    "postbuild": "copyfiles -u 1 -V build/*.js build/fonts/* build/images/* -e build/*.txt ../eggplant-extension/resources/jslibs"
  },
  "keywords": [],
  "dependencies": {
    "@emotion/css": "^11.10.5",
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.11.0",
    "@types/vscode-webview": "^1.57.4",
    "@wso2-enterprise/ui-toolkit": "workspace:*",
    "@wso2-enterprise/eggplant-diagram": "workspace:*",
    "@wso2-enterprise/eggplant-rpc-client": "workspace:*",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vscode-messenger-common": "^0.4.5",
    "vscode-messenger-webview": "^0.4.5"
  },
  "devDependencies": {
    "@types/react": "^18.2.33",
    "@types/react-dom": "^18.2.14",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "@vitejs/plugin-react": "^4.1.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.52.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vite-tsconfig-paths": "^4.2.1",
    "css-loader": "^5.2.7",
    "source-map-loader": "^4.0.1",
    "style-loader": "^1.3.0",
    "ts-loader": "^9.5.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.15.1"
  },
  "author": "wso2",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wso2-enterprise/vscode-extentions.git"
  }
